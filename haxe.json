{
  "build" : {
    "-D"  : ["no-deprecation-warnings"],
    "-cp" : "src/main/haxe",
    "-lib" : [
      "stx_pico",
      "stx_nano",
      "stx_fn",
      "stx_log",
      "stx_asys",
      "stx_bake",
      "stx_om",
      "fletcher"
    ],
    "unit" : {
      "--macro" : [
        "include('stx.config',true)"
      ]
    },
    "main" : {
      "-main" : "Main",
      "--resource" : [
        "src/main/haxe/resource/test.txt@test"
      ],
      "-D" : [
        "stx.config.Resource=test",
        "stx.config.File=src/main/haxe/config/config.txt",
        "stx.config.Env=__STX__CONFIG__CUSTOM_ENV",
        "stx.config.Env=HOME"
      ],
      "interp" : {
        "--interp" : true
      }
    }
  }
}